#include <stdio.h>

/*Crie um programa que lÃª 6 valores inteiros e, em seguida, mostre na tela os valores lidos */

int main(void) {
  int valores[6];
  
  printf("Valores: \n");
  for (int i=1; i<=6; i++){
    scanf("%d",&valores[i]);
  }
  printf("======================\n");
  for (int j=1; j<=6; j++){
    printf("%d\n",valores[j]);
  }
}